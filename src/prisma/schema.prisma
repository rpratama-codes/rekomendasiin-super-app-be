datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum UserRole {
  system_user
  user
}

model User {
  id         String  @id @default(uuid(7))
  first_name String?
  last_name  String?
  email      String  @unique
  password   String?
  username   String?

  @@map("user")
}
