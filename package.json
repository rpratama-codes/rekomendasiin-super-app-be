	"type": "module",
	"main": "index.js",
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"scripts": {
		"dev": "nodemon",
		"build": "rimraf ./dist && rollup --config ./rollup.config.js --configPlugin typescript",
